{
  "name": "@dreamer/test",
  "version": "1.0.0-beta.4",
  "license": "MIT",
  "exports": {
    ".": "./src/mod.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "LICENSE.md",
      "README.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**",
      "package.json"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any",
        "no-unresolved"
      ]
    }
  }
}
