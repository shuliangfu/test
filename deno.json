{
  "name": "@dreamer/test",
  "version": "1.0.12",
  "license": "Apache-2.0",
  "exports": {
    ".": "./src/mod.ts",
    "./browser": "./src/browser/mod.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "src/**/*.json",
      "LICENSE",
      "NOTICE",
      "README.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**",
      "package.json"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any",
        "no-unresolved"
      ]
    }
  },
  "imports": {
    "@dreamer/i18n": "jsr:@dreamer/i18n@^1.0.1",
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@^1.0.16",
    "@dreamer/esbuild": "jsr:@dreamer/esbuild@^1.0.31",
    "@dreamer/logger": "jsr:@dreamer/logger@^1.0.2",
    "playwright": "npm:playwright@1.58.2"
  },
  "nodeModulesDir": "auto",
  "allowScripts": {
    "allow": [],
    "deny": [
      "npm:esbuild@0.27.2",
      "npm:playwright@1.58.2"
    ]
  }
}
