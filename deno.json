{
  "name": "@dreamer/test",
  "version": "1.0.0-beta.35",
  "license": "MIT",
  "exports": {
    ".": "./src/mod.ts"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "LICENSE.md",
      "README.md",
      "TEST_REPORT.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**",
      "package.json"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any",
        "no-unresolved"
      ]
    }
  },
  "imports": {
    "@dreamer/runtime-adapter": "jsr:@dreamer/runtime-adapter@1.0.0-beta.19",
    "@dreamer/esbuild": "jsr:@dreamer/esbuild@^1.0.0-beta.22",
    "@dreamer/logger": "jsr:@dreamer/logger@1.0.0-beta.4",
    "@dreamer/socket-io": "jsr:@dreamer/socket-io@1.0.0-beta.2",
    "puppeteer": "npm:puppeteer@^24.35.0"
  },
  "nodeModulesDir": "auto",
  "allowScripts": {
    "allow": [],
    "deny": [
      "npm:esbuild@0.27.2",
      "npm:puppeteer@24.35.0"
    ]
  }
}
