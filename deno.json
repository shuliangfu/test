{
  "name": "@dreamer/test",
  "version": "1.0.0-alpha.1",
  "license": "MIT",
  "exports": {
    ".": "./src/mod.ts"
  },
  "publish": {
    "include": [
      "src/mod.ts",
      "LICENSE.md",
      "README.md"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.spec.ts",
      "**/test/**",
      "**/tests/**"
    ]
  },
  "lint": {
    "include": [
      "src/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ],
      "exclude": [
        "no-explicit-any"
      ]
    }
  },
  "nodeModulesDir": "auto",
  "imports": {
    "@std/testing/bdd": "jsr:@std/testing@1.0.16/bdd"
  }
}
